    "sass": "^1.54.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.8.0",
    "postcss": "^8.4.16",
